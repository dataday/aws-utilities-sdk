{
    "Statement": [
        {
            "Sid": "AssumeRolePrincipalService",
            "Action": "sts:AssumeRole",
            "Effect": "Allow",
            "Principal": {
                "Service": "ec2.amazonaws.com"
            }
        },
        {
            "Sid": "AssumeRolePrincipalAWS",
            "Action": "sts:AssumeRole",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::123456789012:root"
            },
            "Condition": {
                "Bool": {
                    "aws:MultiFactorAuthPresent": "true"
                }
            }
        }
    ],
    "Version": "2012-10-17"
}
